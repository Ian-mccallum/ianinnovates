{
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/home",
      "destination": "/",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/about",
      "destination": "/about.html"
    },
    {
      "source": "/cv",
      "destination": "/cv.html"
    },
    {
      "source": "/photos",
      "destination": "/photos.html"
    },
    {
      "source": "/portfolio",
      "destination": "/portfolio.html"
    },
    {
      "source": "/testimonials",
      "destination": "/testimonials.html"
    },
    {
      "source": "/contact",
      "destination": "/contact.html"
    },
    {
      "source": "/thank-you",
      "destination": "/thank-you.html"
    }
  ]
}
